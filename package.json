{
  "name": "@peedief/template-engine",
  "version": "0.1.0",
  "description": "A minimal template engine supporting {{ variable }}, {% if %}, and {% for %} syntax patterns",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.MD",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "template",
    "templating",
    "engine",
    "html",
    "rendering"
  ],
  "author": "Claude Code Multi-Agent Team",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  }
}
